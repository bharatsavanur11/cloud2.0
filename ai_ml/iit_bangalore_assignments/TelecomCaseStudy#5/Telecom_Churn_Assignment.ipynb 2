    "# Recharge columns where all the missing value needs to be imputed by 0, for further analysis and modeling.\n",
    "zero_rech_impute = ['total_rech_data_6', 'total_rech_data_7', 'total_rech_data_8', 'total_rech_data_9',\n",
    "        'av_rech_amt_data_6', 'av_rech_amt_data_7', 'av_rech_amt_data_8', 'av_rech_amt_data_9',\n",
    "        'total_rech_amt_6', 'total_rech_amt_7', 'total_rech_amt_8', 'total_rech_amt_9'\n",
    "       ]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 245,
   "metadata": {},
   "outputs": [],
   "source": [
    "churn_data[zero_rech_impute] = churn_data[zero_rech_impute].apply(lambda x: x.fillna(0))"
   ]
