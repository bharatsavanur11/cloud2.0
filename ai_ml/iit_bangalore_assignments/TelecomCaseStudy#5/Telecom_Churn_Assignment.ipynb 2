    "\n",
    "# check sensitivity and specificity\n",
    "print('\\nThe result of logistic regression model with top 30 important features is as following:\\n')\n",
    "sensitivity, specificity, _ = sensitivity_specificity_support(y_test, y_pred, average='binary')\n",
    "print(\"Sensitivity: \\t\", round(sensitivity, 2), \"\\n\", \"Specificity: \\t\", round(specificity, 2), sep='')\n",
    "\n",
    "\n",
    "y_auc = lr.predict_proba(X_test_imp_feature)[:, 1]\n",
    "print(\"AUC:           \", round(roc_auc_score(y_test, y_auc),2))\n",
    "\n",
    "logistic_regression_Top_30_feature = ['logistic_regression_Top_30_feature',round(sensitivity, 2),round(specificity, 2),round(roc_auc_score(y_test, y_auc),2)]"
   ]
