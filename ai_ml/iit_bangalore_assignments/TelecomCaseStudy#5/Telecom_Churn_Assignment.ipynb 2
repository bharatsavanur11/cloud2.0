   "source": [
    "# create box plot for  6th, 7th and 8th month\n",
    "def plot_box(attribute):\n",
    "    plt.figure(figsize=(15,6))\n",
    "    data_churn = churn_filtered\n",
    "    plt.subplot(2,3,1)\n",
    "    sns.boxplot(data=data_churn, y=attribute+\"_6\",x=\"churn\",hue=\"churn\",\n",
    "                showfliers=False,palette=(\"plasma\"))\n",
    "    plt.subplot(2,3,2)\n",
    "    sns.boxplot(data=data_churn, y=attribute+\"_7\",x=\"churn\",hue=\"churn\",\n",
    "                showfliers=False,palette=(\"plasma\"))\n",
    "    plt.subplot(2,3,3)\n",
    "    sns.boxplot(data=data_churn, y=attribute+\"_8\",x=\"churn\",hue=\"churn\",\n",
    "                showfliers=False,palette=(\"plasma\"))\n",
    "    plt.show()"
   ]
  },
