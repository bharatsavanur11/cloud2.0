       "166       std_ic_t2o_mou_6               0.000000\n",
       "167         loc_og_t2o_mou               0.000000"
      ]
     },
     "execution_count": 545,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# predictors\n",
    "features = churn_filtered.drop('churn', axis=1).columns\n",
    "\n",
    "# feature_importance\n",
    "importance = rf_model.feature_importances_\n",
    "\n",
    "# create dataframe\n",
    "feat_importance = pd.DataFrame({'variables': features, 'importance_percentage': importance*100})\n",
    "feat_importance = feat_importance[['variables', 'importance_percentage']]\n",
    "\n",
    "# sort features\n",
    "feat_importance = feat_importance.sort_values('importance_percentage', ascending=False).reset_index(drop=True)\n",
    "print(\"Sum of importance=\", feat_importance.importance_percentage.sum())\n",
    "feat_importance"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Extract top 30 Important feature"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 546,
   "metadata": {},
   "outputs": [],
   "source": [
    "Important_feature = feat_importance['variables'].head(30)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 547,
   "metadata": {},
   "outputs": [],
   "source": [
    "# keep only top 30 feature in train and test dataset and discard other dataset.\n",
    "X_train_imp_feature = X_train.loc[:,Important_feature]\n",
    "\n",
    "X_test_imp_feature = X_test.loc[:,Important_feature]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 548,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Scale the train and test data for logistic regression\n",
    "ss = StandardScaler()\n",
    "X_train_imp_feature = ss.fit_transform(X_train_imp_feature)\n",
    "X_test_imp_feature = ss.transform(X_test_imp_feature)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
